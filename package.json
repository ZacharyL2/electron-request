{
  "name": "electron-request",
  "version": "1.5.0",
  "description": "Zero-dependency, lightweight http request client for electron or nodejs",
  "keywords": [
    "http",
    "https",
    "electron",
    "nodejs",
    "request",
    "curl",
    "fetch",
    "net",
    "network",
    "gzip"
  ],
  "homepage": "https://github.com/islizeqiang/electron-request#readme",
  "bugs": {
    "url": "https://github.com/islizeqiang/electron-request/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/islizeqiang/electron-request.git"
  },
  "license": "MIT",
  "author": "islizeqiang",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rollup -c",
    "release": "npm run build && standard-version"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --ext .js,.ts",
    "*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "code-fabric": "^1.4.1",
    "is-builtin-module": "^3.1.0",
    "lint-staged": "^11.0.0",
    "rollup": "^2.52.2",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-typescript-paths": "^1.3.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-version": "^9.3.0",
    "typescript": "^4.3.4",
    "yorkie": "^2.0.0"
  }
}
